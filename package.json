{
  "name": "eia-overlay",
  "version": "0.0.1",
  "scripts": {
    "dev": "npm run dev:graphics & npm run dev:dashboard",
    "dev:graphics": "parcel watch src/graphics/index.html --public-url ./ --out-dir graphics",
    "dev:dashboard": "parcel watch src/dashboard/index.html --public-url ./ --out-dir dashboard",
    "lint": "eslint ."
  },
  "nodecg": {
    "compatibleRange": ">=0.9",
    "dashboardPanels": [
      {
        "name": "hello-world",
        "title": "Hello World",
        "width": 3,
        "file": "index.html",
        "headerColor": "#9f9bbd"
      }
    ],
    "graphics": [
      {
        "file": "eia-overlay.html",
        "width": 1920,
        "height": 1080
      }
    ]
  },
  "devDependencies": {
    "@types/react-dom": "^17.0.1",
    "@typescript-eslint/eslint-plugin": "^4.16.1",
    "@typescript-eslint/parser": "^4.16.1",
    "bulma": "^0.9.2",
    "eslint": "^7.21.0",
    "eslint-config-standard": "^16.0.2",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.3.1",
    "eslint-plugin-react": "^7.22.0",
    "marquee3000": "^1.1.1",
    "parcel-bundler": "^1.12.4",
    "react-dom": "^17.0.1",
    "typescript": "^4.2.2"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.34",
    "@fortawesome/free-solid-svg-icons": "^5.15.2",
    "@fortawesome/react-fontawesome": "^0.1.14",
    "react": "^17.0.1"
  }
}
